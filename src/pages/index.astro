---
import Home from '@/components/home/index.astro';
import Layout from '@/layouts/layout.astro';
import { SEO } from 'astro-seo';
---

<Layout>
    <SEO
        slot="seo"
        title={'Geetesh Laddha'}
        description={'Geetesh Laddha - Portfolio'}
        openGraph={{
            basic: {
                title: 'Geetesh Laddha',
                type: 'Website',
                image: `${import.meta.env.SITE}/avatar.jpg`,
            },
        }}
    />
    <Home />
</Layout>
