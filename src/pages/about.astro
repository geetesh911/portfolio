---
import Layout from '../layouts/layout.astro';
import AboutComponents from '@/components/about/index.astro';
import { SEO } from 'astro-seo';
---

<Layout>
    <SEO
        slot="seo"
        title={'About - Geetesh Laddha'}
        description={'About - Geetesh Laddha'}
        openGraph={{
            basic: {
                title: 'About - Geetesh Laddha',
                type: 'Website',
                image: `${import.meta.env.SITE}/avatar.jpg`,
            },
        }}
    />
    <AboutComponents />
</Layout>
